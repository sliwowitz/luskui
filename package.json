  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.1",
    "@eslint/js": "^9.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "linkedom": "^0.16.5",
    "prettier": "^3.3.3",
    "typescript-eslint": "^8.13.0",
    "typescript": "^5.6.3"
  }
}
